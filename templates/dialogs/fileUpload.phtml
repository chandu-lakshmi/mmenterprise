<div  style="background:#F0F0F0" class="upload-file-bucket" ng-init="fileUp.uploadFile()">
     <div class="modal-header" ng-style="{'background':fileUp.bucket.color}">
        <h3 class="modal-title text-center" style="color:#fff;padding:2px">{{fileUp.bucket.name | uppercase}}</h3>
    </div>
    <div class="modal-body" style="padding: 5% 10%;">
        <!--qq uploader-->

        <div id="file-uploader" class="bucket-file">       
        <noscript>          
            <p>Please enable JavaScript to use file uploader.</p>
        </noscript>         
        </div>
        <div style="margin-top:60px">
            <ul class="hints">
                <li>Only .xls and .xlsx files are allowed.</li>
                <li>File size must not exceed 25MB.</li>
            </ul>
        </div>
    </div>
    <div class="modal-footer" style="background: #e9e9e9">
     <button class="btn btn-lg" type="button" ng-style='{{fileUp.obj}}' ng-click="$dismiss()" style="border-radius:0;background:#fff">Upload</button>
     <button class="btn btn-link pull-left" style="font-size: 21px;line-height: 35px;outline:0" ng-style='{color:fileUp.bucket.color}' type="button" ng-click="$dismiss()">Cancel</button>
    </div>
</div>